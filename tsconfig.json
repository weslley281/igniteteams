{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@storage/*": ["./src/storage/*"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"]
    },
    "types": ["@types/styled-components-react-native"]
  }
}
